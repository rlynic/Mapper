<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>tk.mybatis</groupId>
  <artifactId>mapper</artifactId>
  <version>4.2.4-SNAPSHOT</version>
  <name>mapper</name>
  <description>Mybatis 通用 Mapper Jar 集成</description>
  <url>https://mybatis.io/mapper-all/mapper</url>
  <licenses>
    <license>
      <name>The MIT License (MIT)</name>
      <url>https://github.com/abel533/mapper/blob/master/LICENSE</url>
    </license>
  </licenses>
  <developers>
    <developer>
      <name>abel533</name>
      <email>abel533@gmail.com</email>
    </developer>
  </developers>
  <scm>
    <connection>scm:git@github.com:abel533/mapper.git/mapper-all/mapper</connection>
    <developerConnection>scm:git@github.com:abel533/mapper.git/mapper-all/mapper</developerConnection>
    <url>git@github.com:abel533/mapper.git/mapper-all/mapper</url>
  </scm>
  <distributionManagement>
    <repository>
      <id>nexus-bingo</id>
      <name>releases</name>
      <url>https://repo.bingosoft.net/repository/releases</url>
    </repository>
    <snapshotRepository>
      <id>nexus-bingo</id>
      <name>snapshots</name>
      <url>https://repo.bingosoft.net/repository/snapshots</url>
    </snapshotRepository>
  </distributionManagement>
  <properties>
    <java.version>17</java.version>
    <maven.compiler.source>17</maven.compiler.source>
    <hsqldb.version>2.5.2</hsqldb.version>
    <jpa.version>2.2</jpa.version>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    <revision>4.2.4-SNAPSHOT</revision>
    <maven.compiler.target>17</maven.compiler.target>
    <slf4j.version>1.7.26</slf4j.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <junit.version>4.13.2</junit.version>
    <logback.version>1.2.11</logback.version>
    <argLine>-Dfile.encoding=UTF-8</argLine>
    <mybatis.version>3.5.11</mybatis.version>
  </properties>
  <dependencies>
    <dependency>
      <groupId>jakarta.persistence</groupId>
      <artifactId>jakarta.persistence-api</artifactId>
      <version>3.1.0</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>tk.mybatis</groupId>
      <artifactId>mapper-core</artifactId>
      <version>4.2.4-SNAPSHOT</version>
      <scope>compile</scope>
      <optional>true</optional>
    </dependency>
    <dependency>
      <groupId>tk.mybatis</groupId>
      <artifactId>mapper-base</artifactId>
      <version>4.2.4-SNAPSHOT</version>
      <scope>compile</scope>
      <optional>true</optional>
    </dependency>
    <dependency>
      <groupId>tk.mybatis</groupId>
      <artifactId>mapper-extra</artifactId>
      <version>4.2.4-SNAPSHOT</version>
      <scope>compile</scope>
      <optional>true</optional>
    </dependency>
    <dependency>
      <groupId>tk.mybatis</groupId>
      <artifactId>mapper-spring</artifactId>
      <version>4.2.4-SNAPSHOT</version>
      <scope>compile</scope>
      <optional>true</optional>
    </dependency>
    <dependency>
      <groupId>tk.mybatis</groupId>
      <artifactId>mapper-weekend</artifactId>
      <version>4.2.4-SNAPSHOT</version>
      <scope>compile</scope>
      <optional>true</optional>
    </dependency>
  </dependencies>
</project>
